{
  "log": {
    "level": "info",
    "timestamp": true
  },
  "dns": {
    "rules": [
      {
        "outbound": "local",
        "server": "local",
        "disable_cache": true,
        "rewrite_ttl": 600,
        "client_subnet": "1.1.1.1/24"
      },
      {
        "clash_mode": "全局代理",
        "server": "remote"
      },
      {
        "clash_mode": "全局直连",
        "server": "local"
      },
      {
        "rule_set": ["geosite-cn"],
        "server": "local"
      },
      {
        "rule_set": ["category-ads-all"],
        "server": "block"
      }
    ],
    "servers": [
      {
        "address": "https://1.1.1.1/dns-query",
        "detour": "节点选择",
        "tag": "remote"
      },
      {
        "address": "https://223.5.5.5/dns-query",
        "detour": "direct",
        "tag": "local"
      },
      {
        "address": "rcode://success",
        "tag": "block"
      }
    ],
    "strategy": "ipv4_only"
  },
  "experimental": {
    "cache_file": {
      "enabled": true
    },
    "clash_api": {
      "external_ui": "/etc/neko/ui/",
      "external_controller": "0.0.0.0:9090",
      "secret": "Akun"
    }
  },
  "inbounds": [
    {
      "domain_strategy": "prefer_ipv4",
      "listen": "127.0.0.1",
      "listen_port": 2334,
      "sniff": true,
      "sniff_override_destination": true,
      "tag": "mixed-in",
      "type": "mixed",
      "users": []
    },
    {
      "type": "tun",
      "address": [
        "172.19.0.1/30",
        "fdfe:dcba:9876::1/126"
      ],
      "route_address": [
        "0.0.0.0/1",
        "128.0.0.0/1",
        "::/1",
        "8000::/1"
      ],
      "route_exclude_address": [
        "192.168.0.0/16",
        "fc00::/7"
      ]
    },
    {
      "tag": "mixed",
      "type": "mixed",
      "listen": "0.0.0.0",
      "listen_port": 1082,
      "sniff": true
    }
  ],
  "outbounds": [
    {
      "tag": "节点选择",
      "type": "selector",
      "outbounds": ["自动选择", "{all}"],
      "filter": [
        {
          "action": "exclude",
          "keywords": [
            "网站", "地址", "剩余", "过期", "时间", "有效", "套餐", "到期", "去除"
          ]
        }
      ]
    },
    {
      "tag": "自动选择",
      "type": "urltest",
      "outbounds": ["{all}"],
      "filter": [
        {
          "action": "exclude",
          "keywords": [
            "网站", "地址", "剩余", "过期", "时间", "有效", "套餐", "到期", "去除"
          ]
        }
      ],
      "url": "http://www.gstatic.com/generate_204",
      "interval": "10m",
      "tolerance": 50
    },
    {
      "tag": "GLOBAL",
      "type": "selector",
      "outbounds": ["节点选择", "自动选择"]
    },
    {
      "type": "dns",
      "tag": "dns-out"
    },
    {
      "type": "direct",
      "tag": "direct"
    },
    {
      "type": "block",
      "tag": "block"
    }
  ],
  "route": {
    "auto_detect_interface": true,
    "rules": [
      {
        "outbound": "dns-out",
        "protocol": "dns"
      },
      {
        "clash_mode": "全局直连",
        "outbound": "direct"
      },
      {
        "clash_mode": "全局代理",
        "outbound": "节点选择"
      },
      {
        "rule_set": ["geosite-cn", "geoip-cn"],
        "outbound": "direct"
      },
      {
        "ip_is_private": true,
        "outbound": "direct"
      },
      {
        "rule_set": ["category-ads-all"],
        "outbound": "block"
      },
      {
        "action": "reject",
        "outbound": "block"
      }
    ],
    "rule_set": [
      {
        "tag": "geosite-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-cn.srs",
        "download_detour": "节点选择"
      },
      {
        "tag": "category-ads-all",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-category-ads-all.srs",
        "download_detour": "节点选择"
      },
      {
        "tag": "geoip-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geoip/rule-set/geoip-cn.srs",
        "download_detour": "节点选择"
      }
    ]
  }
}

